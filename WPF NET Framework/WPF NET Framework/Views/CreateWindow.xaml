<Window x:Class="WPF_NET_Framework.CreateWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_NET_Framework"
        mc:Ignorable="d"
        Title="Cadastro" Height="200" Width="300" Margin="10">
    <StackPanel Margin="20">
        <TextBlock Text="Insira o Nome do Fundo"></TextBlock>
        <TextBox Name="nomeFundo"></TextBox>
        <TextBlock Text="Insira o Código do Fundo"></TextBlock>
        <TextBox Name="codigoFundo"></TextBox>
        <Button x:Name="Enviar" Margin="20" Width="80" Content="Enviar" Click="Enviar_Click"></Button>
    </StackPanel>
</Window>
